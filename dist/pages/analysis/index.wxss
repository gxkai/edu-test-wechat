@import "../../common.wxss";
.introduce {
  padding: 5rpx 25rpx 25rpx 25rpx;
  font-size: 0.6em;
  text-align: center;
}
.name {
  font-size: 2em;
  line-height: 2em;
}
.echarts-title {
  text-align: center;
  font-weight: 700;
  font-size: 2em;
}
.echarts {
  width: 100%;
  height: 600rpx;
}
.score {
  font-size: 1.5em;
}
.score Text {
  color: #f00;
  font-size: 1.5em;
  font-weight: 700;
}
.title {
  text-align: center;
  font-weight: 700;
  font-size: 1.2em;
  margin-top: 25rpx;
}
.result {
  padding: 25rpx;
}
.result-item {
  border: 1rpx solid #eee;
  box-shadow: 0 2rpx 12rpx 0 rgba(0, 0, 0, 0.1);
  padding: 20rpx 20rpx 30rpx 20rpx;
  margin-bottom: 30rpx;
  border-radius: 8rpx;
}
.forum-title {
  text-align: center;
  color: #5ea9fe;
  font-weight: 700;
  font-size: 1.1rem;
  padding: 10rpx 0;
}
.forum-content {
  padding: 5rpx 10rpx;
  text-indent: 2em;
  line-height: 1.3em;
}

