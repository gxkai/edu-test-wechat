.at-timeline-item {
  position: relative;
  padding: 0 0 12rpx;
  /* elements */
  /* modifiers */
}
.at-timeline-item__content {
  margin-left: 40rpx;
  min-height: 56rpx;
  color: #333;
  font-size: 28rpx;
  line-height: 1.5;
  text-align: left;
}
.at-timeline-item__content--sub {
  color: #666;
  font-size: 24rpx;
}
.at-timeline-item__dot, .at-timeline-item__icon {
  position: absolute;
  left: 0;
  top: 5rpx;
  width: 24rpx;
  height: 24rpx;
  font-size: 0;
  text-align: center;
  vertical-align: middle;
  border-radius: 50%;
  background: #FFF;
  box-sizing: border-box;
  z-index: 1;
}
.at-timeline-item__dot .at-icon, .at-timeline-item__icon .at-icon {
  font-size: 24rpx;
}
.at-timeline-item__dot::after {
  content: '';
  display: inline-block;
  box-sizing: border-box;
  width: 24rpx;
  height: 24rpx;
  border: 1PX solid transparent;
  border-radius: 50%;
  border-color: #78A4F4;
}
.at-timeline-item__dot.at-timeline-item__icon::after {
  border-color: transparent;
}
.at-timeline-item__icon {
  color: #78A4F4;
}
.at-timeline-item__tail {
  position: absolute;
  top: 12rpx;
  bottom: -12rpx;
  left: 10rpx;
  border-left: 1PX solid #e2ecf4;
}
.at-timeline-item--green .at-timeline-item__icon {
  color: #13CE66;
}
.at-timeline-item--green .at-timeline-item__dot::after {
  border-color: #13CE66;
}
.at-timeline-item--red .at-timeline-item__icon {
  color: #FF4949;
}
.at-timeline-item--red .at-timeline-item__dot::after {
  border-color: #FF4949;
}
.at-timeline-item--yellow .at-timeline-item__icon {
  color: #FFC82C;
}
.at-timeline-item--yellow .at-timeline-item__dot::after {
  border-color: #FFC82C;
}
.at-timeline .at-timeline-item:last-child .at-timeline-item__tail {
  display: none;
}
.at-timeline--pending .at-timeline-item:nth-last-child(2) .at-timeline-item__content {
  min-height: 80rpx;
}
.at-timeline--pending .at-timeline-item:nth-last-child(2) .at-timeline-item__tail {
  border-left-style: dotted;
}

