@import "../../common.wxss";
Page {
  background: #bde8ff;
}
.item-box {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-around;
  align-content: center;
  padding: 1rem 0.5rem 0.5rem 0.5rem;
}
.scrollview {
  display: flex;
  flex-flow: row nowrap;
  white-space: nowrap;
}
.scrollview .question-item {
  display: inline-block;
  width: 1.6rem;
  height: 1.6rem;
  border: 1rpx solid #000;
  border-radius: 50%;
  text-align: center;
  background: #bde8ff;
  margin-right: 0.4rem;
}
.scrollview .done {
  background: #92c6ef;
}
.question-box {
  background: #fff;
  margin: 0.5rem 1rem 1rem;
  border-radius: 0.5rem;
  border: 1rpx solid #000;
  padding: 0.5rem;
}
.question-title {
  padding: 0.5rem;
  line-height: 1.5em;
}
.answer-box {
  padding: 0.8rem 0.5rem 0.5rem 0.5rem;
}
.answer-item {
  padding: 0.8rem 0.5rem;
  border-radius: 0.4rem;
  border: 1rpx solid #EBEEF5;
  margin-bottom: 0.5rem;
}
.answer-item.checked {
  border: 1rpx solid #92c6ef;
  background: #92c6ef;
}
.prompt {
  position: relative;
  height: 100%;
  background: #f00;
}
.button {
  background: #92c6ef;
  margin: 2rem;
  color: #000;
}

