.at-checkbox {
  position: relative;
  background-color: #FFF;
  position: relative;
  position: relative;
  /* elements */
}
.at-checkbox::before {
  content: '';
  position: absolute;
  -webkit-transform-origin: center;
          transform-origin: center;
  box-sizing: border-box;
  pointer-events: none;
  top: 0;
  left: 0;
  right: 0;
  bottom: auto;
  -webkit-transform: scaleY(0.5);
          transform: scaleY(0.5);
  border-top: 1PX solid #d6e4ef;
}
.at-checkbox::after {
  content: '';
  position: absolute;
  -webkit-transform-origin: center;
          transform-origin: center;
  box-sizing: border-box;
  pointer-events: none;
  top: auto;
  left: 0;
  right: 0;
  bottom: 0;
  -webkit-transform: scaleY(0.5);
          transform: scaleY(0.5);
  border-bottom: 1PX solid #d6e4ef;
}
.at-checkbox__option-wrap {
  padding: 24rpx 32rpx 24rpx 0;
}
.at-checkbox__option-cnt {
  display: flex;
}
.at-checkbox__title {
  display: inline-flex;
  color: #333;
  font-size: 32rpx;
  line-height: 1.5;
  text-align: left;
}
.at-checkbox__icon-cnt {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  margin-top: 4rpx;
  margin-right: 24rpx;
  width: 40rpx;
  min-width: 40rpx;
  height: 40rpx;
  color: transparent;
  font-size: 24rpx;
  line-height: 1;
  border: 2rpx solid rgba(97, 144, 232, 0.2);
  border-radius: 50%;
  background-color: #FFF;
  box-sizing: border-box;
  transition: all 0.2s;
}
.at-checkbox__desc {
  display: block;
  padding-left: 64rpx;
  color: #999;
  font-size: 24rpx;
  text-align: left;
  line-height: 1.5;
  letter-spacing: 0;
}
.at-checkbox__option {
  padding-left: 32rpx;
  transition: background-color 0.3s;
}
.at-checkbox__option:active {
  background-color: #F0F0F0;
}
.at-checkbox__option--disabled .at-checkbox__option-cnt,
.at-checkbox__option--disabled .at-checkbox__desc {
  opacity: 0.3;
}
.at-checkbox__option--disabled:active {
  background-color: #FFF;
}
.at-checkbox__option--selected .at-checkbox__icon-cnt {
  color: #FFF;
  border: none;
  background-color: #6190E8;
}
.at-checkbox__option + .at-checkbox__option .at-checkbox__option-wrap {
  position: relative;
}
.at-checkbox__option + .at-checkbox__option .at-checkbox__option-wrap::after {
  content: '';
  position: absolute;
  -webkit-transform-origin: center;
          transform-origin: center;
  box-sizing: border-box;
  pointer-events: none;
  top: -50%;
  left: -50%;
  right: -50%;
  bottom: -50%;
  border: 0 solid #d6e4ef;
  -webkit-transform: scale(0.5);
          transform: scale(0.5);
  border-top-width: 1PX;
}

